{
    "name": "lekeabiodun/laravel-livewire-sweetalert",
    "description": "Laravel Livewire Sweetalert",
    "keywords": [
        "lekeabiodun",
        "laravel",
        "laravel-livewire-sweetalert"
    ],
    "homepage": "https://github.com/lekeabiodun/laravel-livewire-sweetalert",
    "license": "MIT",
    "authors": [
        {
            "name": "Leke Abiodun",
            "email": "lekepeterabiodun@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.10",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.4",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Lekeabiodun\\SweetAlert\\": "src",
            "Lekeabiodun\\SweetAlert\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lekeabiodun\\SweetAlert\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lekeabiodun\\SweetAlert\\SweetAlertServiceProvider"
            ],
            "aliases": {
                "SweetAlert": "Lekeabiodun\\SweetAlert\\SweetAlertFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
